# Database connection string used by SQLx
DATABASE_URL=postgresql://example-host:5432/example-database
# Append authentication parameters as needed, for example:
# DATABASE_URL=postgresql://example-host:5432/example-database?user=USERNAME&password=PASSWORD

# Maximum number of PostgreSQL connections managed by SQLx
DATABASE_MAX_CONNECTIONS=5

# Port the HTTP server listens on
SERVER_PORT=3000

# Logging configuration for tracing
RUST_LOG=info
